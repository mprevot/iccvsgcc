{
    "editor.tabSize": 4,
    "editor.fontSize": 11,
    "terminal.integrated.fontSize": 11,
    "editor.minimap.enabled": true,
    "editor.minimap.renderCharacters": false,
    "files.trimTrailingWhitespace": true,
    "files.autoSave": "afterDelay",
    "update.channel": "none",
